{
  "name": "gpl-async-storage",
  "version": "1.0.1",
  "description": "一个异步 API 的本地存储库",
  "author": {
    "name": "Eliot Ye",
    "url": "https://github.com/eliot-ye"
  },
  "homepage": "https://github.com/eliot-ye/async-storage",
  "repository": {
    "type": "git",
    "url": "https://github.com/eliot-ye/async-storage.git"
  },
  "keywords": [
    "storage",
    "localstorage",
    "indexedDB",
    "cookie"
  ],
  "type": "module",
  "files": [
    "dist",
    "libs"
  ],
  "main": "./dist/asyncStorage.cjs",
  "module": "./dist/asyncStorage.js",
  "exports": {
    ".": {
      "import": "./dist/asyncStorage.js",
      "require": "./dist/asyncStorage.cjs",
      "types": "./libs/asyncStorage.ts"
    },
    "./engine/cookie": {
      "import": "./dist/cookie.js",
      "require": "./dist/cookie.cjs",
      "types": "./libs/engine/cookie.ts"
    },
    "./engine/localStorage": {
      "import": "./dist/localStorage.js",
      "require": "./dist/localStorage.cjs",
      "types": "./libs/engine/localStorage.ts"
    },
    "./engine/indexedDB": {
      "import": "./dist/indexedDB.js",
      "require": "./dist/indexedDB.cjs",
      "types": "./libs/engine/indexedDB.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "@types/crypto-js": "^4.2.1"
  },
  "dependencies": {
    "crypto-js": "^4.2.0"
  }
}