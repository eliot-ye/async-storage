{
  "name": "gpl-async-storage",
  "version": "1.5.0",
  "description": "一个异步 API 的本地存储库",
  "license": "MulanPSL-2.0",
  "author": {
    "name": "Eliot Ye",
    "url": "https://github.com/eliot-ye"
  },
  "homepage": "https://github.com/eliot-ye/async-storage",
  "repository": {
    "type": "git",
    "url": "https://github.com/eliot-ye/async-storage.git"
  },
  "keywords": [
    "storage",
    "localstorage",
    "indexedDB",
    "cookie"
  ],
  "type": "module",
  "files": [
    "*"
  ],
  "main": "./index.cjs",
  "module": "./index.js",
  "typings": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs",
      "types": "./index.d.ts"
    },
    "./cookie": {
      "import": "./cookie.js",
      "require": "./cookie.cjs",
      "types": "./cookie.d.ts"
    },
    "./localStorage": {
      "import": "./localStorage.js",
      "require": "./localStorage.cjs",
      "types": "./localStorage.d.ts"
    },
    "./indexedDB": {
      "import": "./indexedDB.js",
      "require": "./indexedDB.cjs",
      "types": "./indexedDB.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.1",
    "crypto-js": "^4.2.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.7.0",
    "js-md5": "^0.8.3"
  },
  "dependencies": {}
}